{
    "tabWidth": 4,
    "semi": true,
    "endOfLine": "auto",
    "arrowParens": "avoid",
    "printWidth": 95,
    "importOrder": [
        "^@typings(.*)$",
        "^@middleware(.*)$",
        "^@mongo(.*)$",
        "^@types(.*)$",
        "^@config(.*)$",
        "^@components(.*)$",
        "^@hooks(.*)$",
        "^@utils(.*)$",
        "^@routes/(.*)$",
        "^@jobs/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
